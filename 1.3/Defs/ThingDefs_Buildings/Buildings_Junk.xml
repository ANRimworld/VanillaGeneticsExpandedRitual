<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="BuildingBase">
    <defName>GR_AncientCryofreezer</defName>
    <label>ancient cryofreezer</label>
    <description>A super-charged air-conditioning unit designed for its intended purpose of preventing genomes from breaking down entropically. Unfortunately, that temperature is also beyond the comfort range of almost all organic life but it can no longer maintain the desired output of approaching absolute zero.</description>
    <size>(3,3)</size>
    <fillPercent>0.50</fillPercent>
    <graphicData>
      <texPath>Things/Building/Ruined/AncientCryofreezer</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3, 3)</drawSize>
      <shadowData>
        <volume>(3, 0.35, 3)</volume>
      </shadowData>
    </graphicData>
	<tickerType>Normal</tickerType>
	<costList>
	    <Steel>50</Steel>
		<ComponentIndustrial>3</ComponentIndustrial>
	</costList>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <WorkToBuild>2000</WorkToBuild>
      <Flammability>0.5</Flammability>
    </statBases>
	<comps>
			<li Class="CompProperties_HeatPusher">
				<compClass>CompHeatPusherPowered</compClass>
				<heatPerSecond>-30</heatPerSecond>
				<heatPushMinTemperature>-200</heatPushMinTemperature>
			</li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>150</basePowerConsumption>
			</li>
		</comps>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
  </ThingDef>
  
