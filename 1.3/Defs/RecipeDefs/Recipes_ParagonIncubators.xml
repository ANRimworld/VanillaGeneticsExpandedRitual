	<RecipeDef>
		<defName>GR_CreateParagonBearIncubator</defName>
		<label>paragon bear incubator</label>
		<description>Fill an incubator with paragon genes from a bear</description>
		<jobString>Filling Incubator</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>GR_AnimalEnrichmentCenter</li>	
		</recipeUsers>	
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GR_EmptyIncubator</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_BearSerum</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>GR_OrganicPulp</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>

			<thingDefs>
				<li>GR_EmptyIncubator</li>
				<li>GR_BearSerum</li>
				<li>GR_OrganicPulp</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<GR_ParagonBearIncubator>1</GR_ParagonBearIncubator>
		</products>
		<skillRequirements>
			<Intellectual>5</Intellectual>
		</skillRequirements>
