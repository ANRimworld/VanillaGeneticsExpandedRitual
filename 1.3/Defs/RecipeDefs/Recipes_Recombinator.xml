<Defs>

	<RecipeDef Name="GR_RecombinatorRecipeBase" Abstract="true">
		<jobString>Recombinating genomes</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>3500</workAmount>
		<workSkill>Intellectual</workSkill>
		<recipeUsers>
			<li>GR_GeneRecombinator</li>
		</recipeUsers>
		
		<skillRequirements>
			<Intellectual>6</Intellectual>
		</skillRequirements>
	</RecipeDef>
	<RecipeDef ParentName="GR_RecombinatorRecipeBase">
		<defName>GR_CreateTemplateGenome</defName>
		<label>Create template genome</label>
		<description>Create a template genome that can be used to duplicate existing genome samples.</description>
		<ingredientValueGetterClass>AnimalBehaviours.IngredientValueGetter_Mass</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Corpses</li>
					</categories>
				</filter>
				<count>100</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Corpses</li>
			</categories>
			<disallowedCategories>
				<li>CorpsesMechanoid</li>
			</disallowedCategories>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>CorpsesAnimal</li>
			</categories>
		</defaultIngredientFilter>
		<products>
			<GR_TemplateGenetic>1</GR_TemplateGenetic>
		</products>		
	</RecipeDef>
