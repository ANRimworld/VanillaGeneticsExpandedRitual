				<accuracyShort>1</accuracyShort>
				<accuracyMedium>1</accuracyMedium>
				<accuracyLong>1</accuracyLong>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>GR_FlameBreathAnimated</defaultProjectile>
				<warmupTime>1.5</warmupTime>
				<burstShotCount>1</burstShotCount>
				<minRange>1</minRange>
				<range>15</range>	
				<label>fire breath</label>	  				
				<soundCast>GR_FireBreath</soundCast>
				<muzzleFlashScale>0</muzzleFlashScale>
				<commonality>1</commonality>
			</li>


		</verbs>
		<comps>
			<li Class="AnimalBehaviours.CompProperties_InitialAbility">
				<initialAbility>GR_AdrenalineRush</initialAbility>
			</li>
			
		</comps>
		<race>
			
			<body>QuadrupedAnimalWithPawsAndTail</body>
			<deathActionWorkerClass>DeathActionWorker_BigExplosion</deathActionWorkerClass>
			<executionRange>6</executionRange>
			<predator>true</predator>
			<maxPreyBodySize>2</maxPreyBodySize>
			<fleshType>GR_Normal</fleshType>
			<herdMigrationAllowed>false</herdMigrationAllowed>
			<baseBodySize>0.85</baseBodySize>
			<baseHungerRate>0.18</baseHungerRate>
			<baseHealthScale>0.99</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<gestationPeriodDays>15</gestationPeriodDays>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Intermediate</trainability>
			<wildness>0.75</wildness>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.018</manhunterOnTameFailChance>
			<useLeatherFrom>Wolf_Timber</useLeatherFrom>
			<useMeatFrom>Wolf_Timber</useMeatFrom>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(3, 0.7)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeExpectancy>4</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.45</minAge>
					<soundWounded>Pawn_Dog_Injured</soundWounded>
					<soundDeath>Pawn_Dog_Death</soundDeath>
					<soundCall>Pawn_Dog_Call</soundCall>
					<soundAngry>Pawn_Dog_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
		</tradeTags>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				<statToAdd>
					<li>GR_HybridRole</li>
				</statToAdd>
				<statValues>
					<li>GR_HybridRoleCombat</li>
				</statValues>
				<statDescriptions>
					<li>GR_HybridRoleCombatDesc</li>
				</statDescriptions>
			</li>
		</modExtensions>
	</ThingDef>
