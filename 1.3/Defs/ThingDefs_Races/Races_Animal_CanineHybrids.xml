<Defs>
	<!-- Wolf - Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Wolfbear</defName>
		<label>wolfbear</label>
		<description>A Wolf / Bear hybrid. Very similar to the standard timber wolf, but with better stamina.\n\nUseful abilities: Good guard animal. Butchering gives Wolfbear paws, which can be sold.</description>
		<statBases>
			<MoveSpeed>5.0</MoveSpeed>
			<ComfyTemperatureMin>-30</ComfyTemperatureMin>
			<MarketValue>370</MarketValue>
			<FilthRate>3</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>GR_GoForTheHead</li>
				</capacities>
				<power>6</power>
				<cooldownTime>0.8</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>GR_GoForTheHead</li>
				</capacities>
				<power>6</power>
				<cooldownTime>0.8</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>11</power>
				<cooldownTime>2.1</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>3</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="AnimalBehaviours.CompProperties_InitialAbility">
				<initialAbility>GR_AdrenalineRush</initialAbility>
			</li>
		</comps>
		<race>
			
			<body>QuadrupedAnimalWithPawsAndTail</body>
			<predator>true</predator>
			<fleshType>GR_Normal</fleshType>
			<herdMigrationAllowed>false</herdMigrationAllowed>
			<baseBodySize>1</baseBodySize>
			<baseHungerRate>0.25</baseHungerRate>
			<baseHealthScale>1.5</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<gestationPeriodDays>15</gestationPeriodDays>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Advanced</trainability>
			<wildness>0.75</wildness>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.018</manhunterOnTameFailChance>
			<useLeatherFrom>Wolf_Timber</useLeatherFrom>
			<useMeatFrom>Wolf_Timber</useMeatFrom>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(3, 0.7)</li>
					<li>(3.5, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeExpectancy>4</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.45</minAge>
					<soundWounded>Pawn_Dog_Injured</soundWounded>
					<soundDeath>Pawn_Dog_Death</soundDeath>
					<soundCall>Pawn_Dog_Call</soundCall>
					<soundAngry>Pawn_Dog_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
		</tradeTags>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				<statToAdd>
					<li>GR_HybridRole</li>
				</statToAdd>
				<statValues>
					<li>GR_HybridRoleCombat</li>
				</statValues>
				<statDescriptions>
					<li>GR_HybridRoleCombatDesc</li>
				</statDescriptions>
			</li>
		</modExtensions>
	</ThingDef>
	<PawnKindDef ParentName="AnimalKindBase">
		<defName>GR_Wolfbear</defName>
		<label>wolfbear</label>
		<race>GR_Wolfbear</race>
		<combatPower>75</combatPower>
		<canArriveManhunter>false</canArriveManhunter>
		<ecoSystemWeight>1</ecoSystemWeight>
		<lifeStages>
			<li>
				<label>wolfbear puppy</label>
				<labelPlural>wolfbear puppies</labelPlural>
				<bodyGraphicData>
					<texPath>Things/Pawn/Animal/Wolf/Wolfbear/Wolfbear</texPath>
					<drawSize>2</drawSize>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Animal/Wolf/Wolfbear/Dessicated_Wolfbear</texPath>
					<drawSize>2</drawSize>
				</dessicatedBodyGraphicData>
			</li>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/Animal/Wolf/Wolfbear/Wolfbear</texPath>
					<drawSize>2.5</drawSize>
					<shadowData>
						<volume>(0.25, 0.3, 0.25)</volume>
						<offset>(0,0,-0.15)</offset>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Animal/Wolf/Wolfbear/Dessicated_Wolfbear</texPath>
					<drawSize>2.5</drawSize>
				</dessicatedBodyGraphicData>
			</li>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/Animal/Wolf/Wolfbear/Wolfbear</texPath>
					<drawSize>3.0</drawSize>
					<shadowData>
						<volume>(0.3, 0.4, 0.3)</volume>
						<offset>(0,0,-0.15)</offset>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Animal/Wolf/Wolfbear/Dessicated_Wolfbear</texPath>
					<drawSize>3</drawSize>
				</dessicatedBodyGraphicData>
				<butcherBodyPart>
					<bodyPartGroup>FrontRightPaw</bodyPartGroup>
					<thing>GR_WolfbearPaws</thing>
					<allowFemale>true</allowFemale>
				</butcherBodyPart>
			</li>
		</lifeStages>
		<modExtensions>
			<li Class="GeneticRim.DefExtension_Hybrid">
				<dominantGenome>GR_CanineGenetic</dominantGenome>
				<secondaryGenome>GR_UrsineGenetic</secondaryGenome>
			</li>
		</modExtensions>
	</PawnKindDef>
	<!-- Wolf - Boomalope -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Wolfalope</defName>
		<label>wolfalope</label>
		<description>A Wolf / Boomalope hybrid. A very dangerous predator that tends to explode when confronted with prey too large for them. Friction between the creature's pelt and the boomalope nodules constantly creates static electricity, which is suddenly liberated when the creature dies and the nodules explode.\n\nUseful abilities: Explodes on death in a dangerous electromagnetic pulse (EMP damage). Produces average amounts of chemfuel.</description>
		<statBases>
			<MoveSpeed>5.5</MoveSpeed>
			<ComfyTemperatureMin>-30</ComfyTemperatureMin>
			<MarketValue>370</MarketValue>
			<FilthRate>8</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>5</power>
				<cooldownTime>0.8</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>5</power>
				<cooldownTime>0.8</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				
