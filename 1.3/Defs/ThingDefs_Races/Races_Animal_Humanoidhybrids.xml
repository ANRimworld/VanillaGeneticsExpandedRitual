<Defs>
	<!-- Humanoid - Bear -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>GR_Manbear</defName>
		<label>manbear</label>
		<description>todo.\n\n&lt;color=#E5E54C&gt;Gameplay effect:&lt;/color&gt; Humanoid hybrids will periodically engage other colonists in conversation, talking about a variety of topics. This increases the social recreation needs of those they interact with by 10% (subject to diminishing returns like all joy types). If using Ideology, humanoid hybrids are subject to precepts. Menbear are adept conversationalists and will help in recruiting prisoners.</description>
		<statBases>
			<MoveSpeed>3</MoveSpeed>
			<ComfyTemperatureMin>-10</ComfyTemperatureMin>
			<ComfyTemperatureMax>40</ComfyTemperatureMax>
			<MarketValue>400</MarketValue>
			<FilthRate>2</FilthRate>
			<MeatAmount>20</MeatAmount>
			<LeatherAmount>20</LeatherAmount>
		</statBases>
		<tools>
			<li>
				<label>left fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>2</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>6</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>2</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>6</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>4</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="GeneticRim.CompProperties_HumanoidHybrid">
				
			</li>
		</comps>
		<race>
			<fleshType>GR_HumanMeat</fleshType>
			<herdMigrationAllowed>false</herdMigrationAllowed>
			<body>Monkey</body>
			<baseBodySize>0.35</baseBodySize>
			<baseHungerRate>0.125</baseHungerRate>
			<baseHealthScale>0.35</baseHealthScale>
			<lifeExpectancy>4</lifeExpectancy>
			<gestationPeriodDays>20</gestationPeriodDays>
			<manhunterOnTameFailChance>0.08</manhunterOnTameFailChance>
			<useLeatherFrom>Human</useLeatherFrom>
			<useMeatFrom>Human</useMeatFrom>
			<wildness>0.5</wildness>
			<foodType>OmnivoreAnimal, OvivoreAnimal</foodType>
			<intelligence>Animal</intelligence>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<trainability>Advanced</trainability>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.4</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.8</minAge>
					<soundWounded>Pawn_Monkey_Wounded</soundWounded>
					<soundDeath>Pawn_Monkey_Death</soundDeath>
					<soundCall>Pawn_Monkey_Call</soundCall>
					<soundAngry>Pawn_Monkey_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalGenetic</li>
			<li>AnimalHumanoidHybrid</li>
		</tradeTags>
		<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">
				<statToAdd>
					<li>GR_HybridRole</li>
				</statToAdd>
				<statValues>
					<li>GR_HybridRoleUtility</li>
				</statValues>
				<statDescriptions>
					<li>GR_HybridRoleUtilityDesc</li>
				</statDescriptions>
			</li>
		</modExtensions>
	</ThingDef>
